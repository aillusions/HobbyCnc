<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="cnc.parser" >

    <class name="Vertex"  table="Vertexes" lazy="false">
		<id name="id" column="ID"/>
		<property name="x" not-null="true" column="X"/>
		<property name="y" not-null="true" column="Y" />
		<property name="z" not-null="true" column="Z"  />
		<property name="used" not-null="true" column="USED"/>
		
    </class>
    
</hibernate-mapping>


